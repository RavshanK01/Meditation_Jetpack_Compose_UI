<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Theme.MyApplication" parent="android:Theme.Material.Light.NoActionBar" />

    <!-- Primary brand color. -->
    <item name="colorPrimary" type="color">@color/deepblue</item>
    <item name="colorPrimaryVariant" type="color">@color/deepblue</item>
    <item name="colorOnPrimary" type="color">@color/white</item>
    <!-- Secondary brand color. -->
    <item name="colorSecondary" type="color">@color/deepblue</item>
    <item name="colorSecondaryVariant" type="color">@color/deepblue</item>
    <item name="colorOnSecondary" type="color">@color/black</item>


    <style name="Theme.MyApplication.NoActionBar">
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="Theme.MyApplication.AppBarOverlay" parent="Theme.MyApplication.NoActionBar" />

    <style name="Theme.MyApplication.PopupOverlay" parent="Theme.MyApplication" />

</resources>